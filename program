C Language

#include <stdio.h>

int main() {

int age;

printf("Put your actual age: ");
scanf("%d", &age);

if (age <= 4)
 {
     printf("You are a baby");
 }
else if (age <= 12)
 {
     printf("You are a child");
 }
else if (age <= 18)
 {
     printf("You are young");
 }
else if (age <= 59)
 {
     printf("You are adult");
 }
else
 {
     printf("You are old");
 }
return 0;
}
