print("Put your age: ")

age = input()
age1 = int(age)

if age1 <= 4:
    print("You are a baby")
elif age1 <= 12:
    print("You are a child")
elif age1 <= 18:
    print("You are a young")
elif age1 <= 59:
    print("You are a adult")
else:
    print("You are old")
