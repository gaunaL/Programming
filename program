C++ Language

#include <iostream>
using namespace std;

int main()
{
    int age;
	
	cout << "Put your actual age: ";
    cin >> age;
	
	if (age<=4){
        cout << "You are a baby" << endl;
    }
	else if (age<=12){
        cout << "You are a child" << endl;
    }
	else if (age<=18){
        cout << "You are young" << endl;
    }
	else if (age<=59){
        cout << "You are adult" << endl;
    }
    else {
        cout << "You are old" << endl;
    }

    return 0;
}
----------------------------
Java Switch Case Calculator Double

import java.util.Scanner;

class Calculator {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in); 
        Double sum1, sum2;
	char mathematical;
        
        System.out.print("Please enter the first number to be calculated: ");
        sum1 = input.nextDouble();

	System.out.print("Now enter the second number to be calculated: ");
        sum2 = input.nextDouble();

	System.out.print("Now choose the math operation to be performed: +, -, *, or / ");
	mathematical = input.next().charAt(0);
		
	switch (mathematical){
		
	case '+':
       
        System.out.println(sum1 + sum2);
		
	break;
		
	case '-':
       
        System.out.println(sum1 - sum2);
		
	break;
		
	case '*':
       
        System.out.println(sum1 * sum2);
		
	break;
		
	case '/':
       
        System.out.println(sum1 / sum2);
		
	break;
	}
	input.close();
    }
}
-----------------------
Java Switch Case Calculator Integer

import java.util.Scanner;

class Calculator {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in); 
        int sum1, sum2;
	char mathematical;
        
        System.out.print("Please enter the first number to be calculated: ");
        sum1 = input.nextInt();
	System.out.print("Now enter the second number to be calculated: ");
        sum2 = input.nextInt();

	System.out.print("Now choose the math operation to be performed: +, -, *, or / ");
	mathematical = input.next().charAt(0);
		
	switch (mathematical){
		
	case '+':
       
        System.out.println(sum1 + sum2);
		
	break;
		
	case '-':
       
        System.out.println(sum1 - sum2);
		
	break;
		
	case '*':
       
        System.out.println(sum1 * sum2);
		
	break;
		
	case '/':
       
        System.out.println(sum1 / sum2);
		
	break;
	}
	input.close();
    }
}
---------------------------
HTML

<!DOCTYPE html>
<!--sempre começa com HTML e o DOCTYPE deve ser declarado igual função main-->

<html>
<!--o head é a cabeça da página, title é o nome que aparece na aba-->
<head>
<title>Pagina Web</title>
</head>

<!--abaixo será toda a estrutura que aparece no navegador, equivalente ao } { do java-->

<body>

<!--de h1 a h6 são os titulos, diminuindo conforme aumenta o numero-->
<h1>My First Heading</h1>

<!--no p declaramos o paragrafo-->
<p>My first paragraph.</p>

<!--de h1 a h6 são os titulos, diminuindo conforme aumenta o numero-->
<h2>Hello World!</h2>

<!--dunção a href é para abrir um link-->
<a href="https://www.w3schools.com">This is a link</a>

<!--img src é onde está a imagem, alt é o texto alternativo caso não carregue, width é largura/horizontal e height é altura/vertical-->
<img src="w3schools.jpg" alt="W3Schools.com" width="104" height="142">

<!--br é a quebra de linha-->
<p>This is a <br> paragraph with a line break.</p>

<!--The style attribute is used to add styles to an element, such as color, font, size, and more-->
<p style="color:red;">This is a red paragraph.</p>

</body>
</html>
